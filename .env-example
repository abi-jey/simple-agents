
# Environment variables for simple-agents
# Copy this to .env and fill in your API keys

# Google AI API key (get from https://aistudio.google.com)
GOOGLE_API_KEY=

# OpenAI API key (optional, if using OpenAI)
OPENAI_API_KEY=

# Anthropic API key (optional, if using Claude)
ANTHROPIC_API_KEY=

# Azure Deployed models at https://ai.azure.com/
AZURE_DEPLOYED_MODEL_NAME=<your-model-name>
AZURE_DEPLOYED_MODEL_ENDPOINT=https://<your-resource-name>.services.ai.azure.com/api/projects/<your-project-name>
AZURE_DEPLOYED_MODEL_KEY=<your-azure-deployed-project-key>
