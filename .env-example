
# Environment variables for nagents
# Copy this to .env and fill in your API keys

# OpenAI API key (get from https://platform.openai.com)
OPENAI_API_KEY=

# Anthropic API key (get from https://console.anthropic.com)
ANTHROPIC_API_KEY=

# Google AI API key (get from https://aistudio.google.com)
GOOGLE_API_KEY=

# Azure OpenAI (get from https://ai.azure.com/)
AZURE_DEPLOYED_MODEL_ENDPOINT=https://your-resource.cognitiveservices.azure.com
AZURE_DEPLOYED_MODEL_KEY=your-azure-api-key
AZURE_DEPLOYED_MODEL_NAME=your-deployment-name

# Azure AI Foundry (new project-based)
AZURE_NEW_FOUNDRY_PROJECT_ENDPOINT=https://your-resource.services.ai.azure.com/api/projects/your-project
AZURE_NEW_FOUNDRY_PROJECT_KEY=your-azure-foundry-key
AZURE_NEW_FOUNDRY_PROJECT_MODEL_NAME=your-model-name
